{
  "name": "react-es6-webpack-boilerplate",
  "version": "4.0.0",
  "description": "Boilerplate for kick starting a React Project with ES6 (Babel), Hot reloader using Webpack, Redux and Material-UI.",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "build": "BABEL_ENV=production ./node_modules/.bin/webpack --config webpack.config.production.js",
    "lint": "./node_modules/.bin/eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/laurentfr/react-redux-material-boilerplate.git"
  },
  "keywords": [
    "react",
    "es6",
    "babel",
    "webpack"
  ],
  "author": "Laurent Diaz (http://www.spread3d.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/laurentfr/react-redux-material-boilerplate/issues"
  },
  "homepage": "https://github.com/laurentfr/react-redux-material-boilerplate",
  "dependencies": {
    "babel-polyfill": "^6.7.4",
    "babel-runtime": "^6.3.19",
    "material-ui": "^0.15.0-alpha.2",
    "react": "^0.14.5",
    "react-dom": "^0.14.5",
    "react-redux": "^4.4.1",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "^3.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.3.21",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^0.21.2",
    "eslint-plugin-react": "^2.3.0",
    "file-loader": "^0.8.5",
    "node-sass": "^3.4.2",
    "react-transform-hmr": "^1.0.1",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.8.4",
    "webpack-dev-server": "^1.8.0"
  }
}
